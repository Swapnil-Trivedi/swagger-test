{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Testing API collection"
  },
  "tags": [
    {
      "name": "Users",
      "description": "Endpoints for testing the API"
    },
    {
      "name": "Profile",
      "description": "Endpoints for testing the API"
    }
  ],
  "paths": {
    "/user/{name}": {
      "get": {
        "tags": [
          "Users"
        ],
        "summary": "Fetches a user by name",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "description": "Name of the user",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "User not found"
          }
        }
      }
    },
    "/user/{email}/data": {
      "get": {
        "tags": [
          "Users"
        ],
        "summary": "Fetches a user's data by email",
        "parameters": [
          {
            "name": "email",
            "in": "path",
            "required": true,
            "description": "Email of the user",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "data": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "User not found"
          }
        }
      }
    },
    "/user/{id}": {
      "delete": {
        "tags": [
          "Users"
        ],
        "summary": "Deletes a user by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID of the user to delete",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User deleted successfully"
          },
          "404": {
            "description": "User not found"
          }
        }
      }
    },
    "/profile/{name}/{tag}": {
      "get": {
        "tags": [
          "Profile"
        ],
        "summary": "Fetches a user's profile by name and tag",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "description": "Name of the user",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tag",
            "in": "path",
            "required": true,
            "description": "Tag of the profile",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "tag": {
                      "type": "boolean"
                    },
                    "profileData": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Profile not found"
          }
        }
      }
    },
    "/profile/{name}/{address}": {
      "delete": {
        "tags": [
          "Profile"
        ],
        "summary": "Deletes a user's profile by name and address",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "description": "Name of the user",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "address",
            "in": "path",
            "required": true,
            "description": "Address of the profile",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Profile deleted successfully"
          },
          "404": {
            "description": "Profile not found"
          }
        }
      }
    }
  }
}